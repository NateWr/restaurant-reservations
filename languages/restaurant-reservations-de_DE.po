"\n"
"{user_name}\n"
"{party} people\n"
"{date}\n"
"\n"
"{bookings_link}\n"
"{confirm_link}\n"
"{close_link}\n"
"\n"
"&nbsp;\n"
"\n"
"<em>This message was sent by {site_link} on {current_time}.</em>"
msgstr ""
